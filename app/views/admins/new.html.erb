<link rel="stylesheet/new.css" href="/stylesheets/new.css" media="screen">
<h2 class='animated bounceInDown' id='title'> Register</h2>
<div class="row">
  <div class="col-xs-12 col-sm-6 col-md-5 col-lg-5">
    <form  action="/admin" method="post" enctype="multipart/form-data">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
      <div class='row'>
        <div class="animated bounceInDown col-md-12 col-xs-12" id='adminTitle'></div>
        <h3 class="animated lightSpeedIn col-sm-12 col-xs-12" id='wrench'><span class="glyphicon glyphicon-wrench"></h3>
        <div class="col-xs-6">
          <label for="first_name">First Name</label>
          <input id='first_name' class='form-control input-sm' type="text" name="admin[first_name]" value=""><br>
        </div>
        <div class="col-xs-6">
          <label for="last_name">Last Name</label>
          <input id='last_name' class='form-control input-sm' type="text" name="admin[last_name]" value=""><br>
        </div>
      </div>
      <div class='row'>
        <div class="col-xs-6">
          <label for="email">Email</label>
          <input id='email' class='form-control input-sm' type="text" name="admin[email]" value=""><br>
        </div>
        <div class="col-xs-6">
          <label for='image'>Image</label>
          <input id='image' class='form-control input-sm' type="file" name="admin[image]"><br>
        </div>
      </div>

      <div class='row'>
        <div class="col-xs-6">
          <label for="password">Password</label>
          <input id='password' class='form-control input-sm' type="password" name="admin[password]" value=""><br>
        </div>
        <div class="col-xs-6">
          <label for="password_confirmation">Password Confirmation</label>
          <input id='password_confirmation'class='form-control input-sm' type="password" name="admin[password_confirmation]" value=""><br>
        </div>
      </div>
      <div class='row'>
      <div class="col-xs-12">
        <label for="key">Key</label>
        <input id='key' class='form-control input-sm' type="password" name="admin[key]" value=""><br>
      </div>
      </div>
      <button type="submit" ><span class="glyphicon glyphicon-sunglasses"></span> Register</button>
      <a href="/users">Back to Mainpage</a>
    </form>
  </div>
  <div class="col-xs-9 col-sm-7 col-md-7 col-lg-5">
    <!-- <div class="animated bounceInDown col-md-12 col-xs-12" id='events'></div>
    <h3 class="animated lightSpeedIn col-sm-12 col-xs-12" id='nearYou'>Near You</h3> -->
  </div>
</div><br>
